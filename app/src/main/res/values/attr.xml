<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Medal">
        <attr name="medaltext" format="string"/>
        <attr name="medaldate" format="string"/>
        <attr name="medaltype" format="enum">
            <enum name="lossWeight" value="0"/>
            <enum name="fuce" value="1"/>
            <enum name="monthChampion" value="2"/>
            <enum name="nation" value="3"/>
            <enum name="nation_spec" value="4"/>
        </attr>
        <attr name="medallevel" format="enum">
            <enum name="gold" value="0"/>
            <enum name="silver" value="1"/>
            <enum name="copper" value="2"/>
        </attr>
    </declare-styleable>
    <!-- 自定义控件的属性 -->
    <declare-styleable name="weixin">
        <attr name="normalBitmap" format="reference"></attr>
        <attr name="pitchBitmap" format="reference"></attr>
        <attr name="wx_alpha" format="boolean"></attr>
        <attr name="text" format="string"></attr>
    </declare-styleable>
    <declare-styleable name="Chart">
        <attr name="totleNum" format="float"/>
        <attr name="numSize" format="dimension"/>
        <attr name="unitSize" format="dimension"/>
        <attr name="tipSize" format="dimension"/>
    </declare-styleable>
</resources>