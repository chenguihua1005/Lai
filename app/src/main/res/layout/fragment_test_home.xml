<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/goadapter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go ZillaAdapter" />

    <Button
        android:id="@+id/godb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go DB" />

    <Button
        android:id="@+id/goapi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go API" />

    <Button
        android:id="@+id/gobinding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go Binding" />

    <Button
        android:id="@+id/gozlistview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go ZListView" />

    <Button
        android:id="@+id/govalidate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go Validate" />
</LinearLayout>
